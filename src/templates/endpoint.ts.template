export class {{name}} extends ApiEndpoint {
  public name = "{{path}}";
  public count = {{#if numberOfEntities}}{{numberOfEntities}}{{else}}10{{/if}};
  public definition = {{endpoint.definition}};
  public routes = {
    {{#routes}}
    ['{{path}}']: (req, res, next) => {
      {{#each methods}}
      if (req.method === '{{verb}}') {
        res.status(200).json('IMPLEMENT CUSTOM HANDLING HERE')
      }
      {{/each}}
      next()
    },
    {{/routes}}
  };
}